{
  "name": "react-boilerplate-minimal",
  "version": "0.1.0",
  "description": "",
  "keywords": [
    "react",
    "webpack",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Heisenberg",
  "scripts": {
    "build": "npm run clean-dist && webpack --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "prettier-write": "prettier --write \"{src,tests}/**/*.{js,jsx,css,scss}\"",
    "prettier-check": "prettier --list-different \"{src,tests}/**/*.{js,jsx,css,scss}\"",
    "start": "npm run start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --watchAll --coverage --config=configs/jest.json",
    "sort-import": "sort-import"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@testing-library/react": "^13.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^27.5.1",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-refresh": "^0.12.0",
    "react-router-dom": "^6.3.0",
    "react-test-renderer": "^18.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^5.3.1",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@fluentui/react": "^8.67.2",
    "axios": "^0.27.2",
    "express": "^4.17.3",
    "sort-import": "^1.4.3"
  }
}
